{
    "extends": [
        "next",
        "next/core-web-vitals",
        "prettier",
        "eslint:recommended",
        "plugin:import/recommended",
        "plugin:import/errors"
    ],
    "rules": {
        "no-unused-vars": [
            "error",
            {
                "argsIgnorePattern": "_",
                "varsIgnorePattern": "_"
            }
        ]
    }
}
